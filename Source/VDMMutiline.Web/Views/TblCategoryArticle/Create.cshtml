@model VDMMutiline.SharedComponent.Params.TblCategoryArticleParam
@{
    Layout = null;
}

<div class="row">
    <div class="col-xs-12">
        @using (Html.BeginForm("Create", "TblCategoryArticle", FormMethod.Post, new { @class = "form-horizontal", @data_parsley_type = "", id = "frCreateTblCategoryArticle", @role = "form" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(x => x.TblCategoryArticle.Id)
            <div class="form-group">
                
            </div>
            <div class="form-group">
<label class="col-sm-2 control-label no-padding-right">Id</label>
<div class="col-sm-10">
@Html.TextBoxFor(x => x.TblCategoryArticle.Id, new { @Required = "Required", @class = "col-xs-12 col-sm-12", @placeholder = "" })
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label no-padding-right">IdCate</label>
<div class="col-sm-10">
@Html.TextBoxFor(x => x.TblCategoryArticle.IdCate, new { @Required = "Required", @class = "col-xs-12 col-sm-12", @placeholder = "" })
</div>
</div>
<div class="form-group">
<label class="col-sm-2 control-label no-padding-right">IdArticle</label>
<div class="col-sm-10">
@Html.TextBoxFor(x => x.TblCategoryArticle.IdArticle, new { @Required = "Required", @class = "col-xs-12 col-sm-12", @placeholder = "" })
</div>
</div>

            
           
            <div class="clearfix form-actions">
                <div class="col-md-12 center">
                    <a onclick="submitform()" class="btn btn-info"><i class="ace-icon fa fa-floppy-o bigger-125"></i>Cập nhật</a>
                    <button type="button" class="btn btn-white btn-danger btn-bold" data-dismiss="modal"><i class="ace-icon fa fa-times bigger-125"></i>Đóng</button>
                </div>
            </div>
        }
    </div>
</div>
<script>
    $('.date-picker').datetimepicker({
        weekStart: 1,
        todayBtn: 1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        minView: 2,
        forceParse: 0
    });
</script>