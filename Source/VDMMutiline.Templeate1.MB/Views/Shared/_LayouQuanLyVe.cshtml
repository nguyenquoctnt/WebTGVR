<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="google-site-verification" content="PdonfJRcZsH-duc8ONgZeipFhFzAJVcljNmBRiggXEA" />


    <link href="/Content/Images/logo.png" rel="shortcut icon" type="image/x-icon" />
    <script src="/Content/zipcontents/js/jquery-1.9.1.js"></script>
    <link href="/Content/zipcontents/css/bootstrap.css" rel="stylesheet" />
    <link href="/Content/zipcontents/css/index.css" rel="stylesheet" />
    <link href="/Content/Styles/jquery-ui.min.css" rel="stylesheet" />
    <link href="/Content/zipcontents/css/lunar-datepicker.css" rel="stylesheet" />
    <link href="/Content/Styles/jquery-ui.min.css" rel="stylesheet" />
    <script src="/Content/zipcontents/js/bootstrap.min.js"></script>
    <script src="/Content/zipcontents/js/flight_search_dialog.js"></script>
    <script src="/Content/zipcontents/js/ui.datepicker.lunar.min.js"></script>
    <link href="/Content/VDM/css/style.css" rel="stylesheet" />

    <link href="/Content/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <script src="/Content/js/moment.min.js"></script>
    <link href="/Content/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" />
    <script src="/Content/daterangepicker/daterangepicker.js"></script>
    <script src="/Content/Selecttize/selectize.js"></script>
    <link href="/Content/Selecttize/selectize.bootstrap3.css" rel="stylesheet" />
    <script src="/Content/js/bootstrap-timepicker.min.js"></script>
    <script src="/Content/js/bootstrap-datepicker.min.js"></script>
    <script src="/Content/js/bootstrap-datetimepicker.min.js"></script>
    <script src="/Content/Copy/clipboard.js"></script>
    <link href="/Content/daterangepicker/bootstrap-datepicker3.min.css" rel="stylesheet" />
    <link href="/Content/daterangepicker/bootstrap-timepicker.min.css" rel="stylesheet" />

    <script src="/Content/Popup/jquery.bpopup.js"></script>
    <link href="/Content/Popup/Popubcss.css" rel="stylesheet" />
    <script src="/Scripts/VDMComon.js"></script>
    <link href="/Content/bootstrap-toastr/toastr.css" rel="stylesheet" />
    <script src="/Content/bootstrap-toastr/toastr.min.js"></script>
    <script src="/Content/form-parsley/parsley.js"></script>
    <script>
        window.ParsleyConfig = {
            errorClass: 'has-error'
            , excluded: 'input[type=hidden]'
            , trigger: false
            , animate: true
            , focus: 'first'
            , validationMinlength: 3
            , validators: {}
            , errorElem: '<span></span>'
            , errorsWrapper: '<span class="help-block"></span>'
            , errorTemplate: "<div></div>"
            , classHandler: function (el) {
                return el.$element.closest(".form-control");
            }
        };
    </script>


    @RenderSection("Seometa", required: false)
    <style>
        .form-control {
            margin-bottom: 10px;
        }

        .daterangepicker.show-calendar .calendar {
            display: block;
            width: 100%;
            max-width: unset;
        }

        .daterangepicker .ranges {
            width: 100% !important;
            max-width: unset;
        }

        .daterangepicker button.applyBtn.btn.btn-small.btn-sm.btn-success {
            width: 50%;
        }

        .daterangepicker_start_input, .daterangepicker_end_input {
            width: 50%;
        }

        .daterangepicker input.input-mini {
            width: 100% !important;
            max-width: unset;
        }

        h4.form-section {
            background: #a9a5a5;
            padding: 9px;
            color: #fff;
        }



        .modal-header {
            padding: 10px;
        }

        button.close {
            position: absolute;
            right: 6px;
            top: 18px;
        }

        label.form-control {
            font-weight: normal;
            font-weight: normal;
            border: none;
            box-shadow: none;
        }

        .selectize-input.items.full.has-options.has-items {
            height: 30px !important;
        }
    </style>
</head>
<body>
    @Html.Action("Header", "Control")
    @Html.Action("HeaderDetail", "Control")

    <div class="hidden1 bgSelect" style="margin-top: 46px">
        <div class="row  top log logfix" style="">
            <div class="col-xs-12 cb">
               Quản lý vé đã đặt
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="contentdetail">
            <div class="row" style="background: #e4e4e4;    margin-bottom: 10px;">
                <div class="col-md-12">
                    @Html.Action("Menu", "QuanLyVeDaDat")
                </div>
            </div>
            <div class="col-md-12">
                @RenderBody()
            </div>
        </div>
    </div>
    @RenderSection("scripts", required: false)
    <script src="/Content/form-parsley/parsley.js"></script>
    <script>
        function showmenu() {
            if ($('#menu').css("display") == "block") {
                $('#menu').css("display", "none");
            } else {
                $('#menu').css("display", "block");
            }
        }
    </script>
</body>
</html>
