        $( '#showDate' ).hide();
        $( '#txtReturnDate' ).removeAttr( "required" );

        $( ".mc" ).addClass( "selectway" );
        $( ".kh" ).removeClass( "selectway" );
        $( "#ItineraryType" ).val( "1" );
    }
    function selectedRoundclick() {
        $( "#showDate" ).show();
        $( ".kh" ).addClass( "selectway" );
        $( '#txtReturnDate' ).attr( "required", "required" );
        $( ".mc" ).removeClass( "selectway" );
        $( "#ItineraryType" ).val( "2" );
    }

