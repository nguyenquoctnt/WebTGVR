@model VDMMutiline.SharedComponent.Entities.TblThanhvien
@{
    ViewBag.Title = "Quản lý";
    ViewBag.Description = "Xác nhận duyệt thành viên";
    Layout = null;
}

<div class="row">
    <div class="col-lg-12 mt-15">
      
                @using (Html.BeginForm("Aprove", "TblThanhvien", FormMethod.Post, new { id = "fraproveTblThanhvien", @role = "form" }))
                {
                    @Html.AntiForgeryToken()
                    @Html.HiddenFor(x => x.Id)
                    
                    <p style=" color: red" >@ViewBag.Erro</p>
                    <h5>Bạn có chắc chắn muốn Duyệt thành viên @Model.TenThanhvien?</h5>
                    <a onclick="submitfomrAprove()" class="btn btn-white btn-warning btn-bold" type="submit">
                        Xác nhận
                    </a>
                    <button type="button" class="btn btn-white btn-danger btn-bold" data-dismiss="modal">Đóng</button>
                }
       
    </div>
</div>
