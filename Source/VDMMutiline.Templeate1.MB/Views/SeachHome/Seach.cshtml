<script>

    var $list_departure = $("#list-departure");
    var $list_arrival = $("#list-arrival");
    var $txtDeparture = $("[id$=txtDeparture]");
    var $txtArrival = $("[id$=txtArrival]");
    function SearchAutoCompleteStartPoint1() {
        $("#txtStartPointSearch1").autocomplete({
            source: function (request, response) {
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "@System.Configuration.ConfigurationManager.AppSettings["SearchAirportInSiteUrl"]"+"?key="+ $("#txtStartPointSearch1").val(),
                   success: function ( data ) {
                        response( $.map( data, function ( item ) {
                            var label = "<b>" + item.AirportName + " (" + item.AirportCode + ")</b> <b style='float:right;color:#FF6600;clear:both;white-space: nowrap;'>" + item.CountryName + "</b>"
                            var labeldisplay = item.AirportName + ", " + item.CountryName + " - (" + item.AirportCode + ")";
                            return { labeldisplay: labeldisplay, label: label, value: item.AirportCode };
                        } ) );
                    }
                });
            },
            select: function (event, ui) {
                $('#hdfStartPoint').val(ui.item.value);
                $("#txtDeparture").val(ui.item.labeldisplay);
                $('#txtStartPointSearch1').val('');
                $('#list-departure').css('display', 'none');
                $('#get-diemdi').text(ui.item.labeldisplay);
                $('#list-departure').hide();
            },
            minLength: 3
        });
    }
    function addGeoAirPort_Go(code, name) {
        $("#txtDeparture").val(name);
        $('#hdfStartPoint').val(code);
        $("#get-diemdi").text(name);

        $('#list-departure').hide();

    }
    function SearchAutoCompleteEndPoint1() {
        $("#txtEndPointSearch1").autocomplete({
            source: function (request, response) {
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "@System.Configuration.ConfigurationManager.AppSettings["SearchAirportInSiteUrl"]"+"?key="+ $("#txtEndPointSearch1").val(),
                    dataType: "json",
                    success: function ( data ) {
                        response( $.map( data, function ( item ) {
                            var label = "<b>" + item.AirportName + " (" + item.AirportCode + ")</b> <b style='float:right;color:#FF6600;clear:both;white-space: nowrap;'>" + item.CountryName + "</b>"
                            var labeldisplay = item.AirportName + ", " + item.CountryName + " - (" + item.AirportCode + ")";
                            return { labeldisplay: labeldisplay, label: label, value: item.AirportCode };
                        } ) );
                    }
                });
            },
            select: function (event, ui) {

                $('#hdfEndPoint').val(ui.item.value);
                $("#txtArrival").val(ui.item.labeldisplay);

                $('#get-diemden').text(ui.item.labeldisplay);


                $('#list-arrival').hide();



            },
             minLength: 3
        });
    }
    function addGeoAirPort_To(code, name) {
        $("#txtEndPoint").val(name);
        $('#hdfEndPoint').val(code);
        $("#get-diemden").text(name);
        $('#list-arrival').hide();
    }
</script>
<script>
    function addDays( date, days ) {
        var result = new Date( date );
        result.setDate( date.getDate() + days );
        return result;
    }
    $( document ).ready( function () {
        var mdate = new Date();
        var depart = $( "#txtDepartureDate" ).val();
        if ( depart != "" ) {
            var dsplit = depart.split( "/" );
            mdate = new Date( dsplit[2], dsplit[1] - 1, dsplit[0] );
        }
        var d = new Date();
        var datenew = new Date();

        var strDate = $.datepicker.formatDate( 'dd/mm/yy', d );
        var strDepartureDat = $.datepicker.formatDate( 'dd/mm/yy', addDays( datenew, 3 ) );
        var strReturnDate = $.datepicker.formatDate( 'dd/mm/yy', addDays( datenew, 6 ) );
        $( "#txtDepartureDate" ).val( strDepartureDat );
        $( "#txtReturnDate" ).val( strReturnDate );




        //selectedRoundclick();
        SearchAutoCompleteStartPoint1();
        SearchAutoCompleteEndPoint1();
    } );
