@using VDMMutiline.SharedComponent.Constants
@using VDMMutiline.SharedComponent.EntityInfo
@model VDMMutiline.SharedComponent.Params.BK_BookingParam
@{
    Layout = null;
}
@using (Html.BeginForm("CapNhapghichu", "Booking", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal", @data_parsley_validate = "", id = "frCreate", @role = "form" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(z => Model.Id)
    <div class="form-group row">
        <label class="col-md-2 label-control text-left">Ghi chú</label>
        <div class="col-md-8">
            @Html.TextBoxFor(z => z.note, new { @required = "Required", @class = "form-control", @placeholder = "Ghi chú" })
        </div>
        <div class="col-md-2">
            <button type="submit" class="btn btn-info btn-min-width btn-glow" style=" ">
                <i class="ace-icon fa fa-check bigger-110"></i>Cập nhập
            </button>
        </div>
    </div>

}